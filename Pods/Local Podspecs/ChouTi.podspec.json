{
  "name": "ChouTi",
  "version": "0.5.0",
  "summary": "ChouTi (抽屉) - A framework for Swift development.",
  "description": "ChouTi (抽屉) - A framework for Swift development.\n\nThis is a project includes extended data structures, classes, extensions.",
  "homepage": "https://github.com/Ch0uTi/ChouTi",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "HongHao Zhang": "me@honghaoz.com"
  },
  "source": {
    "git": "https://github.com/Ch0uTi/ChouTi.git",
    "tag": "0.5.0"
  },
  "swift_version": "5",
  "requires_arc": true,
  "default_subspecs": "Core",
  "platforms": {
    "osx": null,
    "ios": null,
    "tvos": null,
    "watchos": null
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/ChouTiTests/**/*.swift",
      "dependencies": {
        "Quick": [
          "~> 2.1"
        ],
        "Nimble": [
          "~> 8.0"
        ]
      }
    }
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Sources/Core/**/*.swift",
      "platforms": {
        "ios": "11.0",
        "tvos": "9.0",
        "watchos": "2.0",
        "osx": "10.12"
      }
    },
    {
      "name": "iOS",
      "ios": {
        "source_files": "Sources/iOS/**/*.swift",
        "exclude_files": [
          "Sources/iOS/UI/LoadingMorphingLabel",
          "Sources/iOS/UI/SlideController"
        ],
        "resource_bundles": {
          "Resources": "Resources/**/*.png"
        },
        "dependencies": {
          "ChouTi/Core": [

          ]
        }
      },
      "platforms": {
        "ios": "11.0"
      }
    },
    {
      "name": "AppExtensionUnsafe",
      "ios": {
        "source_files": "Sources/iOS/UI/SlideController",
        "dependencies": {
          "ChouTi/Core": [

          ]
        }
      },
      "platforms": {
        "ios": "11.0"
      }
    },
    {
      "name": "LoadingMorphingLabel",
      "ios": {
        "source_files": "Sources/iOS/UI/LoadingMorphingLabel",
        "dependencies": {
          "ChouTi/Core": [

          ],
          "LTMorphingLabel": [
            "~> 0.7"
          ]
        }
      },
      "platforms": {
        "ios": "11.0"
      }
    }
  ]
}
